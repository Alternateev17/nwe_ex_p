
  {{#if result}}
  {{#each result}}
  <div class="row">
    <div class="col s6 offset-s3">
      <div class="card">
        <div class="card-image">
        </div>
        <div class="card-content">
          <span class="card-title">Дате: {{Date}}</span>
          <span class="card-title">Название валюты: {{Cur_Name}}</span>
          <span class="card-title">Обривеотура валюты: {{Cur_Abbreviation}}</span>
          <span class="card-title">Сумма: {{Cur_Scale}}</span>
        </div>
        <div class="card-action">
          <span class="card-title">в Белорусских рублях: {{Cur_OfficialRate}} BYN</span>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
  {{/if}}


